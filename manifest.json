{
    "name": "Mute on time",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to mute all tabs on user defined time",
    "icons": {
        "16": "icons/icon.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab",
        "storage"
    ]
}